    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "husky": "^8.0.0",
    "lint-prepush": "^2.2.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "precise-commits": "^1.0.2",
    "prettier": "^2.7.1",
