{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "globalEnv": [
    "DATABASE_URL",
    "DEBUG",
    "ENABLE_MIXPANEL_DEBUG",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "IGNORE_TYPECHECK",
    "MIXPANEL_TOKEN",
    "NEXT_PUBLIC_DEBUG",
    "NEXT_PUBLIC_DEV",
    "NEXT_PUBLIC_EMAIL_CONTACT",
    "NEXT_PUBLIC_FEATURES_ENABLED",
    "NEXT_PUBLIC_HOSTING_URL",
    "NEXT_PUBLIC_OPENAI_API_KEY",
    "NEXT_PUBLIC_SERVER_URL",
    "NEXT_PUBLIC_SIGNIN_MESSAGE",
    "NEXT_PUBLIC_UNOPTIMIZE_IMAGE",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "PORT",
    "SHADOW_DATABASE_URL",
    "SKIP_ENV_VALIDATION",
    "VERCEL_URL",
    "VERCEL"
  ],
  "pipeline": {
    "build": {
      "env": [],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^db:generate"],
      "env": []
    },
    "dev:me": {
      "cache": false,
      "dependsOn": ["^db:generate"],
      "env": []
    },
    "db:generate": {
      "cache": false,
      "env": []
    },
    "lint": {
      "cache": true,
      "env": []
    },
    "migrate:db": {
      "cache": false
    },
    "postinstall": {
      "cache": false,
      "env": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": []
    }
  }
}
