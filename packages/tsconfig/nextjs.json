{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "es2015.collection",
      "es2015.iterable",
      "ES2020"
    ],
    "noEmit": true,
    "outDir": "out"
  },
  "display": "Next.js",
  "exclude": ["node_modules"],
  "extends": "./base.json",
  "include": ["src", "next-env.d.ts"]
}
